		"prefix": "\\exercise",
		"body": [
			"\\begin{exercise}",
			"\t$1",
			"\\end{exercise}"
		]
	},
	"笔记": {
		"prefix": "\\note",
		"body": [
			"\\begin{note}",
			"\t$1",
			"\\end{note}"
		]
	},
	"结论": {
		"prefix": "\\conclusion",
		"body": [
			"\\begin{conclusion}",
			"\t$1",
			"\\end{conclusion}"
		]
	},
	"习题": {
		"prefix": "\\problemset",
		"body": [
			"\\begin{problemset}",
			"\t$1",
			"\\end{problemset}"
		]
	},
	"性质": {
		"prefix": "\\property",
