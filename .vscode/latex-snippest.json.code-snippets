	},
	"图表": {
		"prefix": "\\figure",
		"body": [
			"\\begin{figure}[htbp]",
			"\t\\centering",
			"\t\\includegraphics[width=0.6\\textwidth]{${3:filename}}",
			"\t\\caption{${1:title} \\label{fig:${1:lable}}}",
			"\\end{figure}"
